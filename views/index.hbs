<head>
    <style>
       #map {
        height: 400px;
        width: 100%;
       }
    </style>
</head>

<h1>MTB Street Map</h1>
<div id="map"></div>
    <script>
      function initMap() {
        console.log('ololo')

        var uluru = {lat: -25.363, lng: 131.044};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 4,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD7KYb7KwOqPC-kSF5bCP2--abf-QXTCCA&callback=initMap">
    </script>

